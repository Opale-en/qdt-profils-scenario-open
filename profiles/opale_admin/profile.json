    "rules": [
        {
            "name": "user is admin",
            "description": "Deploy only for windows and user Pierre VIVET",
            "conditions": {
                "all": [
                    {
                        "path": "$.environment.operating_system_code",
                        "value": "win32",
                        "operator": "equal"
                    },
                    {
                        "path": "$.user.name",
                        "value": "PierreVivet-OPALE",
                        "operator": "equal"
                    }
                ]
            }
        }
    ]
